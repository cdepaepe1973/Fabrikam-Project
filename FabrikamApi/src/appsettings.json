{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Provider": "Auto",
    "Description": "Auto-detect database provider: SqlServer if connection string exists, InMemory otherwise"
  },
  "SeedData": {
    "Method": "Json",
    "EnableSeedOnStartup": true,
    "JsonSeedDataPath": "Data/SeedData"
  }
}
