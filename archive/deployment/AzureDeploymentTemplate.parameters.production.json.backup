{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseName": {
      "value": "FabrikamProductionDemo"
    },
    "environment": {
      "value": "dev"
    },
    "githubRepository": {
      "value": "https://github.com/YOUR-USERNAME/Fabrikam-Project"
    },
    "skuName": {
      "value": "B1"
    },
    "databaseProvider": {
      "value": "SqlServer"
    },
    "enableAuthentication": {
      "value": true
    },
    "authenticationMode": {
      "value": "EntraExternalId"
    },
    "enableUserTracking": {
      "value": true
    },
    "entraExternalIdTenant": {
      "value": "YOUR-TENANT-NAME.onmicrosoft.com"
    },
    "entraExternalIdClientId": {
      "value": ""
    },
    "sqlAdminUsername": {
      "value": "fabrikamadmin"
    },
    "enableCustomDomains": {
      "value": false
    },
    "customDomain": {
      "value": ""
    }
  }
}
